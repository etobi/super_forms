<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<ROOT>
		<el>
			<settings.maxNumberOfParticipants>
				<TCEforms>
					<label>LLL:EXT:super_forms/Resources/Private/Language/locallang.xml:tx_superforms_domain_model_processor.configuration_maxNumberOfParticipants</label>
					<config>
						<type>input</type>
						<eval>int</eval>
						<size>5</size>
						<default>999</default>
					</config>
				</TCEforms>
			</settings.maxNumberOfParticipants>
			<settings.textParticipant>
				<TCEforms>
					<label>Text, User ist Teilnehmer</label>
					<config>
						<type>text</type>
						<cols>40</cols>
						<rows>10</rows>
						<wrap>virtual</wrap>
					</config>
					<defaultExtras>
						richtext[*]:rte_transform[mode=ts_css]
					</defaultExtras>
				</TCEforms>
			</settings.textParticipant>
			<settings.textWaitinglist>
				<TCEforms>
					<label>Text, User ist auf der Warteliste</label>
					<config>
						<type>text</type>
						<cols>40</cols>
						<rows>10</rows>
						<wrap>virtual</wrap>
					</config>
					<defaultExtras>
						richtext[*]:rte_transform[mode=ts_css]
					</defaultExtras>
				</TCEforms>
			</settings.textWaitinglist>
		</el>
	</ROOT>
</T3DataStructure>
